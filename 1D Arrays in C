#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x[966],i,sum=0,n;
    scanf("%d\n",&n);
    
    for(i=0;i<n;i++)
    {
     scanf("%d\n",&x[i]);
    }
    for(i=0;i<n;i++)
    {
        sum=sum+x[i];
    }
        printf("%d",sum);
    
    
    return 0;
}
